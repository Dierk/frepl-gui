  native isEditable :: Spinner t -> IO boolean

  native queryAccessibleAttribute :: Spinner t -> AccessibleAttribute -> MutableIO (JArray Object) -> IO Object

  native setEditable :: Spinner t -> boolean -> IO void

  native setValueFactory :: Spinner t -> SpinnerValueFactory t -> IO void

