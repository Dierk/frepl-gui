data ButtonBehavior c = pure native com.sun.javafx.scene.control.behavior.ButtonBehavior where

  pure native new :: c -> ButtonBehavior c
                   | c -> List KeyBinding -> ButtonBehavior c

  native mouseEntered :: ButtonBehavior c -> MouseEvent -> IO void

  native mouseExited :: ButtonBehavior c -> MouseEvent -> IO void

  native mousePressed :: ButtonBehavior c -> MouseEvent -> IO void

  native mouseReleased :: ButtonBehavior c -> MouseEvent -> IO void


